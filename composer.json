{
    "name": "ddeboer/salesforce-mapper-bundle",
    "type": "symfony-bundle",
    "description": "A Salesforce API mapper",
    "keywords": [
        "salesforce",
        "soap",
        "web service"
    ],
    "homepage": "https://github.com/Ddeboer/DdeboerSalesforceMapperBundle/",
    "license": "MIT",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bge-webservices/salesforce-bundle-1"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "phpforce/salesforce-bundle": "dev-master",
        "doctrine/common": "*"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "For the param converter"
    },
    "autoload": {
        "psr-0": {
            "Ddeboer\\Salesforce\\MapperBundle": ""
        }
    },
    "target-dir": "Ddeboer/Salesforce/MapperBundle",
    "minimum-stability": "dev"
}